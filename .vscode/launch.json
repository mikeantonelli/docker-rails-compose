{
	"version": "0.2.0",
	"configurations": [
    {
      "name": "Debug Ruby",
      "type": "Ruby",
      "request": "attach",
      "remotePort": "1234",
      "remoteHost": "127.0.0.1",
      "remoteWorkspaceRoot": "/opt",
      "cwd": "${workspaceRoot}",
      "showDebuggerOutput": true,
      "preLaunchTask": "debug",
      "postDebugTask": "stop debug"
    }
	]
}
